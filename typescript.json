{
  "colums": {
    "prefix": "colums",
    "description": "",
    "body": [
      "import { Button, Tooltip,  Image } from 'antd';",
      "import React from 'react';",
      "import store from '@/store';",
      "",
      "interface Ioptions {",
      "    openRep: (e: any) => void",
      "    openRemark: (e: any) => void",
      "}",
      "",
      "export const columns = ({ openRep, openRemark }: Ioptions) => {",
      "    const { enum: {customerType, orgStatus, orgChannel }}: any = store.getState().dictionarySliceReducer",
      "",
      "    const renderImgs = (text: string[]) => {",
      "       return text?  <Image.PreviewGroup>",
      "           {text?.map(item => {",
      "               return <Image key={item} width={40} src={item}/>",
      "           })}",
      "      </Image.PreviewGroup>: <>-</>",
      "    }",
      "",
      "    return [",
      "        {",
      "            title: '提交时间',",
      "            dataIndex: 'createTime',",
      "            key: 'createTime',",
      "            render: (text: any) => {",
      "                return <>{text ? text : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '机构名称',",
      "            dataIndex: 'orgCname',",
      "            key: 'orgCname',",
      "            render: (text: any) => {",
      "                return <>{text ? text : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '用户姓名',",
      "            dataIndex: 'userCname',",
      "            key: 'userCname',",
      "            render: (text: any) => {",
      "                return <>{text ? text : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '手机号',",
      "            dataIndex: 'phoneNo',",
      "            key: 'phoneNo',",
      "            render: (text: any) => {",
      "                return <>{text ? text : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '问题描述',",
      "            dataIndex: 'content',",
      "            key: 'content',",
      "            ellipsis: true,",
      "            render: (text: any) => {",
      "                return <>{text ? <Tooltip title={text}>",
      "                             <span>{text}</span>",
      "                        </Tooltip> : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '图片',",
      "            dataIndex: 'imgUrlArray',",
      "            key: 'imgUrlArray',",
      "            render: (text: any) => {",
      "                return <>{renderImgs(text)}</>",
      "            }",
      "        },",
      "        {",
      "            title: '回复内容',",
      "            dataIndex: 'replyContent',",
      "            key: 'replyContent',",
      "            ellipsis: true,",
      "            render: (text: any) => {",
      "                return <>{text ? <Tooltip title={text}>",
      "                             <span>{text}</span>",
      "                        </Tooltip> : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '回复图片',",
      "            dataIndex: 'replyImgUrlArrays',",
      "            key: 'replyImgUrlArrays',",
      "            render: (text: any) => {",
      "                return <>{renderImgs(text)}</>",
      "            }",
      "        },",
      "        {",
      "            title: '回复时间',",
      "            dataIndex: 'replyTime',",
      "            key: 'replyTime',",
      "            render: (text: any) => {",
      "                return <>{text ? text : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '备注',",
      "            dataIndex: 'remark',",
      "            key: 'remark',",
      "            ellipsis: true,",
      "            render: (text: any) => {",
      "                return <>{text ? <Tooltip title={text}>",
      "                <span>{text}</span>",
      "           </Tooltip> : '-'}</>",
      "            }",
      "        },",
      "        {",
      "            title: '操作',",
      "            dataIndex: 'content',",
      "            key: 'content',",
      "            width: 100,",
      "            render: (text: string, record: any) => (",
      "                <>",
      "                    <Button type=\"link\" size=\"small\" onClick={() => openRemark(record)} >备注</Button>",
      "                </>",
      "            )",
      "        }",
      "    ]",
      "}",
      "",
      ""
    ]
  }
}